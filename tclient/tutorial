package main

import (
	"context"
	"fmt"
	"xxx/tapi/sharemgnt"

	"github.com/kweaver-ai/idrm-go-common/tclient"
)

func main() {
	var client *sharemgnt.NcTShareMgntClient
	transport, err := tclient.NewTClient(context.Background(), sharemgnt.NewNcTShareMgntClientFactory, &client, "localhost", sharemgnt.NCT_SHAREMGNT_PORT)
	if err != nil {
		fmt.Println(err)
		return
	}
	defer transport.Close()
	result, err := client.GetCSFLevels()
	if err != nil {
		fmt.Println(err)
		return
	}
	fmt.Println(result)
	return
}
